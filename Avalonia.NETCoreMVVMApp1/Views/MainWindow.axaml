<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Avalonia.NETCoreMVVMApp1.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Avalonia.NETCoreMVVMApp1.Views.MainWindow"
        Title="MainWindow">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="300">
        <TextBlock Text="{Binding Greeting}"></TextBlock>
        <Button Content="Open Table" Click="Button_OnClick_Open_Table"></Button>
        <Button Content="Exit" Click="Button_OnClick_Go_Back"></Button>
    </StackPanel>
</Window>