<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Avalonia.NETCoreMVVMApp1.Views.DayTable"
        Title="DayTable">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="300">
        <ListBox Items="{Binding Patients}" SelectedIndex="{Binding SelectedIndex}"></ListBox>
        <Button Content="Add Patient" Click="Button_AddPatient" />
        <Button Content="Go Back" Click="Button_GoBack" />
    </StackPanel>
</Window>